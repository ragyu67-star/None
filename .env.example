# .env.example

# Solana Trading Bot Configuration

# Wallet private key
WALLET_PRIVATE_KEY=your_private_key_here

# RPC URL for Solana
RPC_URL=https://api.mainnet-beta.solana.com

# Trading parameters
TRADING_PAIR=SOL/USDC
BUY_PRICE=100.0
SELL_PRICE=120.0
TRADE_AMOUNT=1.0

# Optional parameters
# Set to true to enable logging
ENABLE_LOGGING=true
# Set this to your desired log level (info, debug, warn, error)
LOG_LEVEL=info
