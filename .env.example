# ===========================================
# MISTY VOLUME BOT - ENVIRONMENT CONFIGURATION
# ===========================================

# ===========================================
# WALLET CONFIGURATION (REQUIRED)
# ===========================================

# Your wallet private key in base58 format
# IMPORTANT: Never share this key or commit it to version control!
# Get from: Phantom, Solflare, or other Solana wallet
PRIVATE_KEY=your_wallet_private_key_here

# ===========================================
# NETWORK CONFIGURATION (REQUIRED)
# ===========================================

# Solana RPC endpoint URL
# Options:
# - Public: https://api.mainnet-beta.solana.com (free, rate limited)
# - Alchemy: https://solana-mainnet.g.alchemy.com/v2/YOUR-API-KEY
# - QuickNode: https://your-endpoint.solana-mainnet.quiknode.pro/TOKEN/
# - Helius: https://rpc.helius.xyz/?api-key=YOUR-API-KEY
RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Solana network environment
# Options: mainnet-beta, devnet, testnet
NETWORK=mainnet-beta

# ===========================================
# TRADING CONFIGURATION (REQUIRED)
# ===========================================

# Token address to trade (SPL token mint address)
# Example: So11111111111111111111111111111111111111112 (Wrapped SOL)
TOKEN_ADDRESS=your_token_address_here

# Number of wallets to use for trading
# More wallets = more distributed trading activity
# Recommended: 5-20 for organic appearance
WALLET_COUNT=10

# Buy ratio percentage (0-100)
# This determines buy vs sell distribution
# Example: 70 = 70% buys, 30% sells
# Recommended: 60-80 for volume generation
BUY_RATIO=70

# ===========================================
# TRADING PARAMETERS (OPTIONAL)
# ===========================================

# Minimum trade amount in SOL
MIN_TRADE_AMOUNT=0.01

# Maximum trade amount in SOL
MAX_TRADE_AMOUNT=1.0

# Minimum delay between trades (milliseconds)
TRADE_DELAY_MIN=5000

# Maximum delay between trades (milliseconds)
TRADE_DELAY_MAX=15000

# Priority fee in SOL for faster transaction processing
PRIORITY_FEE=0.0001

# ===========================================
# JITO CONFIGURATION (OPTIONAL)
# ===========================================

# Enable Jito MEV protection
# Set to 'true' to enable, 'false' to disable
JITO_ENABLED=false

# Jito tip amount in SOL
# Higher tips = higher priority in Jito bundles
JITO_TIP_AMOUNT=0.001

# Jito block engine URL
JITO_BLOCK_ENGINE=https://mainnet.block-engine.jito.wtf

# ===========================================
# SERVER CONFIGURATION (OPTIONAL)
# ===========================================

# Web dashboard port
PORT=5000

# Server host (0.0.0.0 for all interfaces, localhost for local only)
HOST=0.0.0.0

# ===========================================
# LOGGING CONFIGURATION (OPTIONAL)
# ===========================================

# Log level: error, warn, info, debug
LOG_LEVEL=info

# Enable file logging
LOG_TO_FILE=true

# Log file path
LOG_FILE_PATH=./logs/bot.log

# ===========================================
# MONITORING (OPTIONAL)
# ===========================================

# Enable performance monitoring
MONITORING_ENABLED=true

# Stats collection interval (milliseconds)
STATS_INTERVAL=1000

# ===========================================
# ADVANCED SETTINGS (OPTIONAL)
# ===========================================

# Maximum retries for failed transactions
MAX_RETRIES=3

# Transaction confirmation timeout (milliseconds)
CONFIRMATION_TIMEOUT=60000

# Enable transaction simulation before sending
SIMULATE_TRANSACTIONS=true

# Slippage tolerance percentage
SLIPPAGE_TOLERANCE=1.0
